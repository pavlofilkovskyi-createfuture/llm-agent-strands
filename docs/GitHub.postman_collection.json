				{
					"name": "1. init",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Read the 'mcp-session-id' header from the response",
									"const sessionId = pm.response.headers.get('mcp-session-id');",
									"if (sessionId) {",
									"    pm.collectionVariables.set('mcp-session-id', sessionId);",
									"    pm.test('mcp-session-id header is present and set as collection variable', function () {",
									"        pm.expect(sessionId).to.be.a('string').and.not.empty;",
									"    });",
									"} else {",
									"    pm.test('mcp-session-id header is missing in the response', function () {",
									"        pm.expect(sessionId).to.not.be.undefined;",
									"    });",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
